   <rBody wit="CAO-C CAO-E CAO-M CAO-V CAO-H CAO-R CAO-D CAO-F CAO-S CAO-L"> Fundata est domus domini
      <app>
         <rdg wit="CAO-C CAO-E CAO-M CAO-V CAO-H CAO-R CAO-D CAO-F CAO-S"> super verticem</rdg>
         <rdg wit="CAO-L"> supra verticem</rdg>
      </app> montium et
      <app>
         <rdg wit="CAO-C CAO-E CAO-M CAO-V CAO-H CAO-R CAO-D CAO-F CAO-S"> exaltata</rdg>
         <rdg wit="CAO-L"> elevata</rdg>
      </app> est
      <app>
         <rdg wit="CAO-M CAO-V CAO-H CAO-R CAO-F CAO-S CAO-L"> super omnes colles</rdg>
         <rdg wit="CAO-C CAO-E CAO-D"> super colles</rdg>
      </app> et venient
      <app>
         <rdg wit="CAO-C CAO-E CAO-M CAO-V CAO-H CAO-R CAO-F CAO-S CAO-L"> ad eam</rdg>
         <rdg wit="CAO-D"> ad eum</rdg>
      </app> omnes gentes et dicent Gloria tibi
      <app>
         <rdg wit="CAO-C CAO-E CAO-M CAO-H CAO-R CAO-D CAO-F CAO-S CAO-L"> domine</rdg>
         <rdg wit="CAO-V"> Deus</rdg>
      </app>
   </rBody>
   <verses>
      <vBody type="r" wit="CAO-E CAO-V CAO-H CAO-R"> Venientes autem venient cum exsultatione portantes manipulos suos</vBody>
      <vBody type="r" wit="CAO-M"> Venientes autem</vBody>
      <vBody id="c6756-1" type="r" num="2" wit="CAO-C"> Venientes autem</vBody>
      <rep corresp="c6756-1" type="r" wit="CAO-C">
