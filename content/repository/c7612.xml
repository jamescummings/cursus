            <rdg wit="CAO-D CAO-S CAO-L"> sanguinem</rdg>
            <rdg wit="CAO-F"> sanguis</rdg>
         </app>
         <app>
            <rdg wit="CAO-L"> alii vero</rdg>
            <rdg wit="CAO-D CAO-S"> alii vero duo</rdg>
            <rdg wit="CAO-F"> alii autem</rdg>
