<ant previd="c2820" nextid="c2822" id="c2821">
   <aBody wit="CAO-S">
      <verse type="stanza">
         <l part="N">Exsultet tellus laetentur et insulae</l>
         <l part="N">Christo regnante davidico germine</l>
         <l part="N">quem virgo sacra puritatis gremio</l>
         <l part="N">prolatum mundo edidit castissimo</l>
         <l part="N">Maria cuius celebre solemnium</l>
         <l part="N">plebs veneratur ubique fidelium</l>
      </verse>
   </aBody>
