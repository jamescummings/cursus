   <rBody wit="CAO-M CAO-H CAO-S CAO-L">
      <app>
         <rdg wit="CAO-M CAO-S CAO-L"> Peccante me quotidie</rdg>
         <rdg wit="CAO-H"> Peccantem me quotidie</rdg>
      </app>
      <app>
         <rdg wit="CAO-L"> et non</rdg>
         <rdg wit="CAO-S"> non</rdg>
         <rdg wit="CAO-M"> et ne</rdg>
         <rdg wit="CAO-H"> et nec</rdg>
      </app>
      <app>
