         <rdg wit="CAO-M"> me paenitet</rdg>
         <rdg wit="CAO-S"> repaenitentem</rdg>
      </app> timor mortis
     <app>
         <rdg wit="CAO-M CAO-H CAO-L"> conturbat me</rdg>
